<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="testsuite" verbose="1" >

    <groups>
        <run>
            <include name="sanity"></include>
        </run>
    </groups>
    <listeners>
        <listener class-name="com.tricentis.demowedbshop.customelisteners.CustomListeners"/>
        <listener class-name="org.uncommons.reportng.HTMLReporter"></listener>
        <listener class-name="org.uncommons.reportng.JUnitXMLReporter"></listener>
    </listeners>
    <test name="Login Test" >
        <classes>
            <class name="com.tricentis.demowedbshop.testsuite.ComputerPageTest"/>
            <class name="com.tricentis.demowedbshop.testsuite.RegisterPageTest"/>
            <class name="com.tricentis.demowedbshop.testsuite.LoginPageTest"/>
        </classes>
    </test>
</suite>